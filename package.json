    "node-sass": "4.14.1",
    "prettier": "^2.2.1",
    "react-scripts": "^4.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{html,css,scss,md,json,yaml}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx}": [
      "prettier --write",
      "git add"
    ]
