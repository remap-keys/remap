{
  "name": "Remap",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@types/ajv": "^1.0.0",
    "ajv": "^7.0.3",
    "firebase": "^8.2.1",
    "immer": "^8.0.0",
    "notistack": "^1.0.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "stackdriver-errors-js": "^0.8.0",
    "typescript": "^4.1.3",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start-env": "sh -ac '. ./.env.${REACT_APP_ENV}; npm run gen-types; react-scripts start'",
    "start-dev": "REACT_APP_ENV=dev npm run start-env",
    "start": "npm run start-dev",
    "build-env": "sh -ac '. ./.env.${REACT_APP_ENV}; npm run gen-types; react-scripts build'",
    "build-dev": "REACT_APP_ENV=dev npm run build-env",
    "build": "npm run build-dev",
    "build-production": "REACT_APP_ENV=production npm run build-env",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write './src/**/*.{ts,tsx}'",
    "gen-types": "json2ts src/services/storage/assets/keyboard-definition-schema.json --style.singleQuote > src/gen/types/KeyboardDefinition.ts;"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.167",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.14",
    "@types/react-router-dom": "^5.1.6",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "babel-eslint": "^10.1.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.21.5",
    "husky": "^4.3.6",
    "json-schema-to-typescript": "^10.1.2",
    "lint-staged": "^10.5.3",
    "node-sass": "4.14.1",
    "prettier": "^2.2.1",
    "react-scripts": "4.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{html,css,scss,md,json,yaml}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx}": [
      "prettier --write",
      "git add"
    ]
  }
}
